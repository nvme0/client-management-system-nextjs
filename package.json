{
  "name": "client-management-system-nextjs",
  "version": "0.0.1",
  "author": "Jeff Summers",
  "description": "A production-ready Client Management System to assist Personal Service Professionals (i.e. personal trainers, dog trainers, coaching, mentors, freelancers) in tracking client information.",
  "bugs": {
    "url": "https://github.com/nvme0/client-management-system-nextjs/issues"
  },
  "homepage": "https://github.com/nvme0/client-management-system-nextjs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nvme0/client-management-system-nextjs.git"
  },
  "scripts": {
    "clean:prebuild": "rm -rf ./.worker",
    "clean:build": "rm -rf .next/*",
    "prebuild:build": "tsc --project ./service-worker/tsconfig.worker.json",
    "prebuild": "npm run clean:prebuild && npm run prebuild:build",
    "build": "npm run prebuild && npm run clean:build && next build",
    "develop": "npm run clean:build && next dev",
    "develop:sw": "tsc --watch --preserveWatchOutput --project ./service-worker/tsconfig.worker.json",
    "start": "next start",
    "db:save": "prisma migrate save --experimental",
    "db:up": "prisma migrate up --experimental",
    "db:gen": "prisma generate",
    "clean:codegen": "rm -rf ./gql/__generated__",
    "apollo:codegen": "apollo client:codegen --target typescript --excludes=node_modules/* --includes=gql/**/*.ts --localSchemaFile=schema.gql --outputFlat gql/__generated__ --passthroughCustomScalars --customScalarsPrefix Gql",
    "gql:gen": "npm run clean:codegen && npm run apollo:codegen",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@chakra-ui/core": "^1.0.0-rc.2",
    "@chakra-ui/theme": "^1.0.0-rc.2",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@prisma/client": "^2.4.1",
    "apollo-server-micro": "^2.16.1",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "cookie": "^0.4.1",
    "emotion-theming": "^10.0.27",
    "faker": "^4.1.0",
    "formik": "^2.1.5",
    "graphql": "^15.3.0",
    "graphql-request": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "localforage": "^1.9.0",
    "mysql": "^2.18.1",
    "next": "^9.5.1",
    "next-google-fonts": "^1.2.1",
    "next-pwa": "^3.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-particles-js": "^3.3.0",
    "react-query": "^2.5.13",
    "react-table": "^7.5.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0-rc.3",
    "uuid": "^8.3.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@prisma/cli": "^2.4.1",
    "@storybook/addon-actions": "^6.0.10",
    "@storybook/addon-essentials": "^6.0.10",
    "@storybook/addon-links": "^6.0.10",
    "@storybook/react": "^6.0.10",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.4.0",
    "@types/faker": "^4.1.12",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-table": "^7.0.22",
    "@types/uuid": "^8.0.1",
    "@types/yup": "^0.29.4",
    "apollo": "^2.30.2",
    "apollo-server-testing": "^2.16.1",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "jest": "^26.2.2",
    "jest-next-dynamic": "^1.0.1",
    "react-is": "^16.13.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
